---
interface Props {
  className?: string;
}
const { className } = Astro.props;
---

<script type="module" src="https://unpkg.com/@splinetool/viewer@1.0.68/build/spline-viewer.js"></script>
<spline-viewer width="50%" height="50%" class={className} loading-anim-type="spinner-small-dark" url="https://prod.spline.design/bTRwdvH1zLw0tP7z/scene.splinecode"></spline-viewer>
